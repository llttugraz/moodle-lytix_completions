{{!
  @template lytix_completions/activity_completion_csv

  Activity completion csv.

  The purpose of this template is to render an csv file.

  Classes required for JS:
  * lytix_completions/activity_completion

  Data attributes required for JS:
  * none

  Context variables required for this template:
  * none

  Example context (json):
  {
      "statistic_label_activity": "Quiz",
      "statistic_label_name": "Quiz 1",
      "statistic_label_total": "30",
      "statistic_label_done": "1",
      "statistic_label_open": "29"
  }

}}
{{#str}}statistic_label_activity, lytix_completions{{/str}};{{#str}}statistic_label_name, lytix_completions{{/str}};{{#str}}statistic_label_total, lytix_completions{{/str}};{{#str}}statistic_label_done, lytix_completions{{/str}};{{#str}}statistic_label_open, lytix_completions{{/str}}
{{#entries}}
{{{type}}};{{{name}}};{{{total}}};{{{done}}};{{{open}}}
{{/entries}}
